{
    "name": "chaloman/tonal-php",
    "description": "Paquete tonal",
    "type": "library",
    "require-dev": {
        "pestphp/pest": "^4.1",
        "laravel/pint": "^1.26",
        "phpstan/phpstan": "^2.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Chaloman\\Tonal\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Gonzalo Guevara",
            "email": "gonzarock@yahoo.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "lint": "pint",
        "lint:test": "pint --test",
        "analyse": "phpstan analyse",
        "test": "pest"
    }
}
