{
    "name": "chaloman/tonal",
    "description": "Paquete tonal",
    "type": "library",
    "require-dev": {
        "pestphp/pest": "4.x-dev",
        "laravel/pint": "dev-main",
        "phpstan/phpstan": "2.1.x-dev"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Chaloman\\Tonal\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Gonzalo Guevara",
            "email": "gonzarock@yahoo.com"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "lint": "pint",
        "lint:test": "pint --test",
        "analyse": "phpstan analyse",
        "test": "pest"
    }
}
